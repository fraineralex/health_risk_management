<odoo>
    <!-- Action of the wizard -->
    <record id="action_ars_export_wizard" model="ir.actions.act_window">
        <field name="name">Exportar Reporte ARS</field>
        <field name="res_model">ars.export.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

    <!-- Inherit from account.menu_finance_reports menu -->
    <record id="menuitem_ars_export" model="ir.ui.menu">
        <field name="name">Exportar ARS</field>
        <field name="parent_id" ref="account.account_reports_management_menu"/>
        <field name="action" ref="ars_template_export.action_ars_export_wizard"/>
        <field name="sequence" type="int">5</field>
    </record>
</odoo>
